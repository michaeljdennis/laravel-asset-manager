{
    "name": "michaeljdennis/phasset",
    "description": "A simple PHP Asset Library.",
    "keywords": ["asset", "assets"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dennis",
            "email": "michaeljdennis@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Phasset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phasset\\PhassetServiceProvider"
            ],
            "aliases": {
                "Phasset": "Phasset\\Facades\\Laravel\\Phasset"
            }
        }
    },
}
